{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "strict": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "noEmit": true
    },
    "include": [
        "src"
    ]
}